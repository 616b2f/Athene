@model Athene.Inventory.Web.Areas.Librarian.Models.RentViewModels.RentedViewModel
@{
    ViewBag.Title = Localizer["Rented Books"];
    Layout = "_Layout";
}
<h3>@ViewBag.Title</h3>
@Html.Partial("_UserPreview", Model.User)
<h3>@Localizer["Rented Books"]</h3>
@foreach (var item in Model.RentedItems)
{
    @Html.Partial("_RentedInventoryItemPreview", item)
}
