@model BookItem

<div class="row">
    <div class="col-md-12">
        <label asp-for="StockLocation"></label>
        @Model.StockLocation.OneLiner
        <label>Status</label>
        @if (Model.RentedBy == null)
        {
            @:Nicht ausgeliehen
        }
        else
        {
            @:Ausgeliehen
        }
    </div>
</div>
